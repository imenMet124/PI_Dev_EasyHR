<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="tn.esprit.formations.controllers.AjouterQuizController">
    <children>
        <Label layoutX="50.0" layoutY="20.0" text="Add New Quiz" />
        <TextField fx:id="titleField" layoutX="50.0" layoutY="60.0" promptText="Title" prefWidth="500.0" />
        <Button layoutX="400.0" layoutY="360.0" onAction="#handleAjouter" text="Add" />
        <Button layoutX="500.0" layoutY="360.0" onAction="#handleCancel" text="Cancel" />
    </children>
</AnchorPane>